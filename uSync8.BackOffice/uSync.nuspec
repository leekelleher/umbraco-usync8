<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>uSync</id>
    <version>8.0.0-alpha58</version>
    <title>uSync for Umbraco 8</title>
    <authors>Kevin Jump</authors>
    <owners>Jumoo</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Database syncing for Umbraco</description>
    <summary></summary>
    <releaseNotes><![CDATA[8.0 - Release for Umbraco 8
8.1 - Compatability Release for Umbraco 8.1
8.2 - uSync Complete Companion release.
8.3 - Language Rename support for Umbraco 8.3
8.4 - Dependency checking updates
8.5 - Updates for uSync Console
8.6 - Clean Export, Missing Parent Support]]>
    </releaseNotes>
    <dependencies>
      <dependency id="uSync.Core" version="$depends$"/>
      <dependency id="Microsoft.AspNet.SignalR.Core" version="2.4.0"/>
    </dependencies>
    <tags>umbraco usync</tags>
    <repository type="git" url="https://github.com/KevinJump/uSync8" />
  </metadata>
  <files>
    <file src="..\readme.txt" target="readme.txt" />
    <file src="Targets\**\*.*" target="build" />
    <file src="bin\$Configuration$\uSync8.BackOffice.dll" target="lib\net472" />
    <file src="Config\**\*" target="content\Config" />
    <file src="App_Plugins\uSync8\**\*.*" target="content\App_Plugins\uSync8" />
  </files>
</package>